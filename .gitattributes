# Auto detect text files and perform LF normalization
* text=auto
*.html filter=lfs diff=lfs merge=lfs -text
*.pickle filter=lfs diff=lfs merge=lfs -text
*.csv filter=lfs diff=lfs merge=lfs -text
data/datasets/combined_dataset.csv filter=lfs diff=lfs merge=lfs -text
data/datasets/qqp.csv filter=lfs diff=lfs merge=lfs -text
data/datasets/sts.csv filter=lfs diff=lfs merge=lfs -text
